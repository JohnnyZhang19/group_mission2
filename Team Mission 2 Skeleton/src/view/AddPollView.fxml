<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AddPollController">
   <children>
      <Button fx:id="addPoll" layoutX="162.0" layoutY="285.0" mnemonicParsing="false" onAction="#addPoll" text="Add Poll">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Button>
      <Button fx:id="clear" layoutX="57.0" layoutY="285.0" mnemonicParsing="false" onAction="#clear" text=" Clear ">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding></Button>
      <ChoiceBox fx:id="selectPoll" layoutX="158.0" layoutY="216.0"  prefWidth="150.0" />
      <Label fx:id="selectLabel" layoutX="21.0" layoutY="223.0" text="Select poll placement: " />
      <Label fx:id="pollNameLabel" layoutX="27.0" layoutY="130.0" text="Name of poll to add: " />
      <TextField fx:id="enterPollName" layoutX="148.0" layoutY="123.0" onKeyTyped="#keyTyped" promptText="enter new poll name" />
   </children>
</AnchorPane>
